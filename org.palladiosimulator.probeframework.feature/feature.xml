<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.probeframework.feature"
      label="ProbeFramework Feature"
      version="2.0.0.qualifier"
      provider-name="palladiosimulator.org"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="http://palladiosimulator.org/">
      Allows the specification of measuring points by decorating EMF
models. The measuring at specified points is supported by the
ProbeFramework framework.
   </description>

   <copyright>
      This plugin is protected by copyright held by the members of
the Palladio Research Group, University of Karlsruhe.
   </copyright>

   
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.jscience" version="4.1.3" match="greaterOrEqual"/>
      <import plugin="org.apache.log4j" version="1.2.13" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.palladiosimulator.commons" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.metricspec" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.measurementframework" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.probeframework" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.util"/>
   </requires>

   <plugin
         id="org.palladiosimulator.probeframework"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.probeframework.probes.example"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.jscience"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
