<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.palladiosimulator.probespec.presentation.probespecModelWizardID"
            name="%_UI_probespecModelWizard_label"
            class="org.palladiosimulator.probespec.presentation.probespecModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/probespecModelFile.gif">
         <description>%_UI_probespecModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.palladiosimulator.probespec.presentation.probespecEditorID"
            name="%_UI_probespecEditor_label"
            icon="icons/full/obj16/probespecModelFile.gif"
            extensions="probespec"
            class="org.palladiosimulator.probespec.presentation.probespecEditor"
            contributorClass="org.palladiosimulator.probespec.presentation.probespecActionBarContributor">
      </editor>
   </extension>

</plugin>
